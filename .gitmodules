# Prefer to use https URLs instead of ssh, so that the computer where i clone my dotfiles does not need to have SSH keys set up to my Github account.
[submodule ".local/repos/sandboxd"]
	path = .local/repos/sandboxd
	url = https://github.com/benvan/sandboxd.git
[submodule ".local/repos/dircolors-solarized"]
	path = .local/repos/dircolors-solarized
	url = https://github.com/seebi/dircolors-solarized.git
[submodule ".local/repos/tmux-colors-solarized"]
	path = .local/repos/tmux-colors-solarized
	url = https://github.com/seebi/tmux-colors-solarized.git
[submodule ".local/repos/fzf-marks"]
	path = .local/repos/fzf-marks
	url = https://github.com/urbainvaes/fzf-marks.git
[submodule ".local/repos/dotbot"]
	path = .local/repos/dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule ".local/repos/dotbot-plugins/ifplatform"]
	path = .local/repos/dotbot-plugins/ifplatform
	url = https://github.com/erikw/dotbot-ifplatform.git
	branch = fix-module-import
