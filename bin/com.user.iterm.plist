<?xml version="1.0" encoding="UTF-8"?>
<!-- Start iterm tmux session on login. -->
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.user.iterm</string>
        <key>ProgramArguments</key>
        <array>
            <string>/Users/erikw/bin/iterm.sh</string>
            <string>irctor</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
    </dict>
</plist>
