hi -a CompTitle                    font-weight: bold; font-family: Lucida Grande,Arial; font-size: 14px; text-align: left;
hi -a CompTitle>*                  padding-left: 40px;
hi -a CompTitleSep								 height: 0px;

hi -a CompItem										 color: #2b2b2b;
hi -a CompItem                     text-shadow: none; font-weight: normal; font-size: 18px; font-family: Lucida Grande,Arial;
hi -a CompItem:nth-child(2n+1)     background: inherit;

hi -a CompItem[selected]					 color: #2b2b2b;
hi -a CompItem[selected]           background: -moz-linear-gradient(bottom, #3873B3, #3873B3) repeat scroll 0 0 transparent;

hi -a CompIcon                     display: inline-block; width: 16px; min-width: 16px;
hi -a CompIcon                     margin-right: 2ex; margin-left: 2px;

hi -a CompDesc										 color: #2b2b2b; width: 50%; font-family: Lucida Grande, Arial !important;
hi -a CompResult									 width: 50%; overfloaw: hidden;
"separator content rule
hi -a CompResult									 border-right: 2px solid;
hi -a CompResult									 -moz-border-right-colors: #D7D7D7 #000;

hi -a CompGroup                    color: #2b2b2b; font-weight:normal; background: transparent;
hi -a CompGroup                    border-top:   1px solid #D7D7D7;


hi -a CompLess										 text-align: center; height: 0; line-height: .5ex; padding-top: 1ex;
hi -a CompMore										 text-align: center; height: .5ex; line-height: .5ex; margin-bottom: -.5ex;
hi -a CompMsg											 background: transparent;
hi -a CompMsg											 font-style: Ubuntu,Verdana;font-size: 18px;
"hi -a CompMore::after		
"hi -a CompLess::after                  
"hi -a CompIcon>img
"hi -a CompItem>*                  
"NOTE: style arrow 
"hi -a CompGroup:last-of-type           color: red !important;
"hi -a CompGroup:not(:first-of-type)
"NOTE: gradient
hi -a Gradient				          height: 1px; margin-bottom: -1px; margin-top: -1px;
hi -a GradientLeft							background-color: #2f2f2f;
hi -a GradientRight							background-color: #2f2f2f;
"NOTE: link
hi -a Link                      color: #2b2b2b !important;
hi -a Link:not(:hover)>LinkInfo color: #2b2b2b !important;
hi -a Message                   color: #2b2b2b !important;
hi -a Message\ String           color: #2b2b2b !important;
hi -a LinkInfo		              color: #2b2b2b !important;

hi -a URL				    text-decoration: none !important; color: #2b2b2b; background: inherit; font-weight:normal !important; font-family: Verdana; text-shadow: none !important;
hi -a URL:hover			text-decoration: underline; cursor: pointer;
